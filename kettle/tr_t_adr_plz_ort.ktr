    <quoteString>N</quoteString>
    <sql>DELETE FROM av2gb_work.t_adr_plz_ort WHERE gem_bfs &#x3d; &#x3f;&#x3b;&#xa;&#xa;INSERT INTO av2gb_work.t_adr_plz_ort &#x28;lokalisationsname, hausnummer, plz, zusatzziffern, ortschaft, gebaeudeeingang_von, status, inaenderung, attributeprovisorisch, istoffiziellebezeichnung, lage, hoehenlage, im_gebaeude, gwr_egid, gwr_edid, lokalisationsart, gem_bfs&#x29;&#xa;&#xa;WITH adr AS &#x28;&#xa;SELECT ein.&#x2a;, lokname.text as atext, lok.art as lokalisationsart&#xa;FROM av_avdpool_ch.gebaeudeadressen_gebaeudeeingang as ein, av_avdpool_ch.gebaeudeadressen_lokalisationsname as lokname, av_avdpool_ch.gebaeudeadressen_lokalisation as lok&#xa;WHERE ein.gem_bfs &#x3d; &#x3f;&#xa;AND lokname.gem_bfs &#x3d; &#x3f;&#xa;AND lok.gem_bfs &#x3d; &#x3f;&#xa;AND ein.gebaeudeeingang_von &#x3d; lokname.benannte&#xa;AND lok.tid &#x3d; ein.gebaeudeeingang_von&#xa;&#x29;,&#xa;ort AS &#x28;&#xa; SELECT os.flaeche, osname.atext&#xa; FROM plz_ortschaft.plzortschaft_ortschaft as os, plz_ortschaft.plzortschaft_ortschaftsname as osname, av_avdpool_ch.gemeindegrenzen_gemeindegrenze as gem&#xa; WHERE gem.gem_bfs &#x3d; &#x3f;&#xa; AND ST_Intersects&#x28;gem.geometrie, os.flaeche&#x29;&#xa; AND os.t_id &#x3d; osname.ortschaftsname_von&#xa; AND os.status IN &#x28;0,1&#x29;&#xa; AND os.inaenderung &#x3d; 1&#xa;&#x29;,&#xa;plz AS &#x28;&#xa; SELECT plz, zusatzziffern, flaeche&#xa; FROM plz_ortschaft.plzortschaft_plz6 as plz, av_avdpool_ch.gemeindegrenzen_gemeindegrenze as gem&#xa; WHERE gem.gem_bfs &#x3d; &#x3f;&#xa; AND ST_Intersects&#x28;gem.geometrie, plz.flaeche&#x29;&#xa; AND plz.status IN &#x28;0,1&#x29;&#xa; AND plz.inaenderung &#x3d; 1&#xa;&#x29;&#xa;SELECT adr.atext, adr.hausnummer, plz.plz, plz.zusatzziffern, ort.atext, adr.gebaeudeeingang_von, adr.status, &#xa;       adr.inaenderung, adr.attributeprovisorisch, adr.istoffiziellebezeichnung, adr.lage, adr.hoehenlage, &#xa;       adr.im_gebaeude, adr.gwr_egid, adr.gwr_edid, &#xa;       adr.lokalisationsart, adr.gem_bfs&#xa;FROM adr, ort, plz&#xa;WHERE adr.gem_bfs &#x3d; &#x3f;&#xa;AND ST_Intersects&#x28;adr.lage, ort.flaeche&#x29;&#xa;AND ST_Intersects&#x28;adr.lage, plz.flaeche&#x29;&#xa;ORDER BY ort.atext, adr.atext, adr.hausnummer&#x3b;</sql>
